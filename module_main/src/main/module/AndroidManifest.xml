<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jason.module_main" >


    <application android:theme="@style/AppTheme"
        android:name="debug.MyApplication"
        android:label="@string/app_name">

        <activity android:name=".mvp.ui.activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.ui.activity.GuideActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".mvp.ui.activity.MainActivity"
            android:screenOrientation="portrait"/>

        <service android:name=".service.KeepAliveService"
            android:exported="false"/>
        <activity android:name=".mvp.ui.activity.WebActivity"
            android:screenOrientation="portrait"/>

    </application>
</manifest>
